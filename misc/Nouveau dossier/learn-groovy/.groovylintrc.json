{
    "extends": "recommended",
    "rules": {
        "CatchException": {
            "enabled": false
        },
        "CompileStatic": {
            "enabled": false
        },
        "DuplicateNumberLiteral": {
            "enabled": false
        },
        "DuplicateStringLiteral": {
            "enabled": false
        },
        "ImplicitClosureParameter": {
            "enabled": false
